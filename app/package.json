    "apisauce": "^1.1.1",
    "date-fns": "^2.11.1",
    "immer": "^6.0.3",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "ramda": "^0.27.0",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-gesture-handler": "^1.6.0",
    "react-native-modal-datetime-picker": "^8.5.4",
    "react-native-modal-dropdown": "^0.7.0",
    "react-native-reanimated": "^1.8.0",
    "react-native-safe-area-context": "^0.7.3",
