    "@storybook/react-native": "^5.3.17",
    "babel-jest": "24.9.0",
    "eslint": "^5.0.0",
    "eslint-plugin-flowtype": "3.13.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "flow-bin": "^0.105.2",
    "flow-typed": "^3.0.0",
    "husky": "^4.2.3",
    "jest": "24.9.0",
    "lint-staged": "^10.0.8",
    "metro-react-native-babel-preset": "^0.58.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "16.9.0",
    "reactotron-core-client": "2.5.0",
    "reactotron-react-native": "4.0.3",
    "reactotron-redux": "^3.1.2",
    "reactotron-redux-saga": "^4.2.3",
    "snazzy": "^8.0.0",
    "standard": "^14.3.1",
    "typescript": "^3.7.2"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "beforeAll",
      "describe",
      "test",
      "it",
      "jest",
      "expect",
      "fetch",
      "navigator",
      "__DEV__",
      "XMLHttpRequest",
      "FormData",
      "React$Element"
    ],
    "ignore": [
      "flow-typed/",
      "ignite/Examples"
    ]
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "standard --fix",
      "git add"
    ]
  },
  "concurrent": false
